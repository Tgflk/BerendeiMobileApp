<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityMenu"
    android:background="#442727">


    <ImageView
        android:id="@+id/iV_logo"
        android:layout_width="181dp"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_svg_our" />

    <ImageView
        android:id="@+id/infa"
        android:layout_width="138dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_logo"
        app:srcCompat="@drawable/infor_svg" />

    <ImageView
        android:id="@+id/iV_Home"
        android:layout_width="103dp"
        android:layout_height="103dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infa"
        app:srcCompat="@drawable/ellipse_home" />

    <TextView
        android:id="@+id/textView_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/pt_serif_bold"
        android:text="Уютные домики"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_Home"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_mainhome"
        android:layout_width="118dp"
        android:layout_height="33dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="#738241"
        android:fontFamily="@font/pt_serif"
        android:text="подробнее"
        android:textSize="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_Home"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/iV_bitch"
        android:layout_width="103dp"
        android:layout_height="103dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="130dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_Home"
        app:srcCompat="@drawable/ellipse_bitch" />

    <TextView
        android:id="@+id/tV_bitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/pt_serif_bold"
        android:text="Шикарный пляж"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_bitch" />

    <Button
        android:id="@+id/button_bitch"
        android:layout_width="118dp"
        android:layout_height="33dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#738241"
        android:fontFamily="@font/pt_serif"
        android:text="подробнее"
        android:textSize="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_bitch"
        tools:ignore="TouchTargetSizeCheck,TextSizeCheck" />

    <ImageView
        android:id="@+id/iV_banya"
        android:layout_width="103dp"
        android:layout_height="103dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="132dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_bitch"
        app:srcCompat="@drawable/ellipse_inf" />

    <TextView
        android:id="@+id/tV_banya"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/pt_serif_bold"
        android:text="Информация о нас"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_banya" />

    <Button
        android:id="@+id/button_onas"
        android:layout_width="118dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#738241"
        android:fontFamily="@font/pt_serif"
        android:text="подробнее"
        android:textSize="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_banya"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/iV_besedka"
        android:layout_width="103dp"
        android:layout_height="103dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infa"
        app:srcCompat="@drawable/tlips_besedka" />

    <TextView
        android:id="@+id/tV_besedka"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="58dp"
        android:fontFamily="@font/pt_serif_bold"
        android:text="Беседки"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_besedka" />

    <Button
        android:id="@+id/button_besedka"
        android:layout_width="118dp"
        android:layout_height="33dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="30dp"
        android:backgroundTint="#738241"
        android:fontFamily="@font/pt_serif"
        android:text="подробнее"
        android:textSize="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_besedka"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/iV_infor"
        android:layout_width="103dp"
        android:layout_height="103dp"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iV_besedka"
        app:srcCompat="@drawable/ellipse_banya" />

    <TextView
        android:id="@+id/tV_infor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="36dp"
        android:fontFamily="@font/pt_serif_bold"
        android:text="Русские бани"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_infor" />

    <Button
        android:id="@+id/btn_mainbanya"
        android:layout_width="118dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#738241"
        android:fontFamily="@font/pt_serif"
        android:text="подробнее"
        android:textSize="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.928"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iV_infor"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/btn_user_main1"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="16dp"
        android:background="#442727"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/reg_svg"
        tools:ignore="SpeakableTextPresentCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>